<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Tools Plus Plugin wx3.3.x (64 bit)" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel33_64/share/codeblocks/plugins/ToolsPlus" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs33_64/plugins/contrib/ToolsPlus" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="../../../devel33_64/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="$(#CB_RELEASE_TYPE)" />
				</Compiler>
				<Linker>
					<Add library="codeblocks" />
					<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
					<Add library="gdi32" />
					<Add directory="../../../devel33_64" />
					<Add directory="$(#WX33_64.lib)/gcc_dll$(WX_CFG)" />
				</Linker>
			</Target>
			<Environment>
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="33" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-D_WIN64" />
			<Add option="-DcbDEBUG" />
			<Add option="-Winvalid-pch" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-std=gnu++11" />
			<Add directory="../../../include" />
			<Add directory="../../../include/tinyxml" />
			<Add directory="../../../sdk/wxscintilla/include" />
			<Add directory="$(#WX33_64.include)" />
			<Add directory="$(#WX33_64.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
		</Compiler>
		<Linker>
			<Add option="-mthreads" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ..\..\..\devel33_64\share\codeblocks\ToolsPlus.zip manifest.xml" />
			<Add after="update33_64.bat" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="CmdConfigDialog.cpp" />
		<Unit filename="CmdConfigDialog.h" />
		<Unit filename="PipedProcessCtrl.cpp" />
		<Unit filename="PipedProcessCtrl.h" />
		<Unit filename="Resources/dialogs.fbp" />
		<Unit filename="ShellCtrlBase.cpp" />
		<Unit filename="ShellCtrlBase.h" />
		<Unit filename="ToolsPlus.cpp" />
		<Unit filename="ToolsPlus.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="se_globals.cpp" />
		<Unit filename="se_globals.h" />
		<Unit filename="shellproperties.cpp" />
		<Unit filename="shellproperties.h" />
		<Unit filename="update32" />
		<Unit filename="update33_64.bat" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
